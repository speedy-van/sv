{
  "name": "@speedy-van/utils",
  "version": "1.0.0",
  "description": "Utility functions and helpers for Speedy Van",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
