{
  "name": "@speedy-van/app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prebuild": "prisma generate --schema=../../packages/shared/prisma/schema.prisma",
    "dev": "next dev -H 0.0.0.0",
    "dev:clean": "pnpm run clean:cache & pnpm run dev",
    "clean:cache": "powershell -Command \"Remove-Item -Recurse -Force .next -ErrorAction SilentlyContinue; New-Item -ItemType Directory -Force .next/server; Set-Content -Path .next/server/pages-manifest.json -Value '{}'; Set-Content -Path .next/server/app-paths-manifest.json -Value '{}'\"",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p ${PORT:-3000}",
    "postinstall": "prisma generate --schema=../../packages/shared/prisma/schema.prisma",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=../../packages/shared/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy",
    "db:seed": "tsx scripts/seed.ts",
    "db:create-apple-driver": "tsx scripts/create-apple-test-driver.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:e2e": "playwright test e2e/",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:observability": "playwright test e2e/customer-portal-observability.spec.ts",
    "test:telemetry": "tsx scripts/test-telemetry.ts",
    "test:qa-acceptance": "tsx scripts/run-qa-acceptance.ts",
    "test:admin-acceptance": "playwright test e2e/admin-dashboard-acceptance.spec.ts",
    "test:admin-performance": "playwright test e2e/admin-performance.spec.ts",
    "test:admin-security": "playwright test e2e/admin-security.spec.ts",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:telemetry && npm run test:qa-acceptance",
    "test:stripe-config": "tsx scripts/test-stripe-config.ts",
    "verify:stripe-production": "tsx scripts/verify-stripe-production.ts",
    "get-stripe-account-id": "tsx scripts/get-stripe-account-id.ts",
    "setup:stripe": "tsx scripts/setup-stripe.ts",
    "telemetry:test": "tsx scripts/test-telemetry.ts",
    "release:check": "tsx scripts/release-check.ts",
    "release:deploy": "tsx scripts/release-deploy.ts",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true npm run build && open bundle-analysis.html",
    "performance:build": "npm run build",
    "performance:start": "npm run start",
    "performance:lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "performance:booking-luxury": "npx lighthouse http://localhost:3000/booking-luxury --output=html --output-path=./lighthouse-booking-luxury-report.html",
    "seo-check": "tsx scripts/seo-check.ts",
    "sitemap": "next-sitemap",
    "bundle:analyze": "npm run analyze",
    "bundle:check": "npx @next/bundle-analyzer",
    "test:schema": "playwright test tests/schema-validator.spec.ts",
    "generate:favicons": "node generate-favicons.js ./public/favicon-base.png ./public",
    "rag:build": "tsx scripts/build-rag.ts",
    "rag:flush": "tsx scripts/build-rag.ts --flush"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.6",
    "@chakra-ui/react": "^2.10.9",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@faker-js/faker": "^10.0.0",
    "@heroicons/react": "^2.2.0",
