    "glob": "^11.0.3",
    "ioredis": "^5.8.0",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.540.0",
    "mapbox-gl": "^3.14.0",
    "next": "^14.2.33",
    "next-auth": "^4.24.11",
    "next-sitemap": "^4.2.3",
    "node-cron": "^3.0.3",
    "openai": "^6.6.0",
    "otplib": "^12.0.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "recharts": "3.1.2",
    "resend": "^6.2.2",
    "schema-dts": "^1.1.5",
    "stripe": "^15.12.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^18.19.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.2",
    "@types/react": "^18.2.45",
    "@types/react-dom": "18.2.22",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.31",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prisma": "6.17.1",
    "sharp": "^0.34.3",
    "tsx": "^4.20.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}
