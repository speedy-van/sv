        ],
        "xAxes": [
          {
            "label": "Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_response_time_ms_count{status!=\"200\"}[5m])",
