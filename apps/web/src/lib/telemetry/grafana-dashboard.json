        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "xAxes": [
          {
            "label": "Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
