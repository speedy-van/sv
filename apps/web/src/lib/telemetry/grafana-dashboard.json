{
  "dashboard": {
    "id": null,
    "title": "Speedy Van - Performance Dashboard",
    "tags": ["speedy-van", "performance", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_response_time_ms_sum[5m]) / rate(api_response_time_ms_count[5m])",
            "legendFormat": "Average Response Time (ms)"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (ms)",
            "min": 0
          }
        ],
        "xAxes": [
          {
            "label": "Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_response_time_ms_count[5m])",
            "legendFormat": "Requests per second"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "xAxes": [
          {
            "label": "Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Database Query Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(db_query_time_ms_sum[5m]) / rate(db_query_time_ms_count[5m])",
            "legendFormat": "Average Query Time (ms)"
          }
        ],
        "yAxes": [
          {
            "label": "Query Time (ms)",
            "min": 0
          }
        ],
        "xAxes": [
          {
            "label": "Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_heap_used",
            "legendFormat": "Heap Used (bytes)"
          },
          {
            "expr": "memory_heap_total",
            "legendFormat": "Heap Total (bytes)"
          },
          {
            "expr": "memory_rss",
            "legendFormat": "RSS (bytes)"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (bytes)",
            "min": 0
